#include<stdio.h>
int main()
{
	//输入三个整数x,y,z，请把这三个数由小到大输出。
	int x,y,z;
	printf("输入三个数字：");
	scanf("%d%d%d",&x,&y,&z);
	if(x>y&&y>z)
	{
		printf("%d,%d,%d",x,y,z);
	}
	if(x>z&&z>y)
	{
		printf("%d,%d,%d",x,z,y);
	}
	if(y>z&&z>x)
	{
		printf("%d,%d,%d",y,z,x);
	}
	if(y>x&&x>z)
	{
		printf("%d,%d,%d",y,x,z);
	}
	if(z>x&&x>y)
	{
		printf("%d,%d,%d",z,x,y);
	}
	if(z>y&&y>x)
	{
		printf("%d,%d,%d",z,y,x);
	}
	if(x==y||y==z||x==z||x==y==z)
	{
		printf("你是故意输入相等的数的！快滚！");
	}
	return 0;	
}
